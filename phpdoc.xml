<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:schemaLocation="https://www.phpdoc.org https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd"
>
    <title>Service Booking - PHP Documentation</title>
    <paths>
        <output>public/developer-docs/php</output>
        <cache>.phpdoc/cache</cache>
    </paths>
    <version number="1.0.0">
        <api>
            <source dsn=".">
                <path>app</path>
                <path>Modules</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>vendor/**/*</path>
                <path>storage/**/*</path>
                <path>bootstrap/**/*</path>
                <path>public/**/*</path>
                <path>resources/**/*</path>
                <path>database/**/*</path>
                <path>tests/**/*</path>
                <path>node_modules/**/*</path>
            </ignore>
            <default-package-name>ServiceBooking</default-package-name>
        </api>
    </version>
    <template name="default"/>
</phpdocumentor>
